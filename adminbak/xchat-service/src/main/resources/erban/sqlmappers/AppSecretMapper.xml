<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.erban.main.mybatismapper.AppSecretMapper" >
  <resultMap id="BaseResultMap" type="com.erban.main.model.AppSecret" >
    <id column="os" property="os" jdbcType="INTEGER" />
    <result column="sign_key" property="signKey" jdbcType="VARCHAR" />
    <result column="app_version" property="appVersion" jdbcType="VARCHAR" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
  </resultMap>

  <sql id="Base_Column_List" >
    os, sign_key, app_version, create_time
  </sql>

  <select id="selectList" resultMap="BaseResultMap" parameterType="com.erban.main.model.AppSecret" >
    select 
    <include refid="Base_Column_List" />
    from app_secret_key
  </select>

  <delete id="deleteByPrimaryKey" parameterType="com.erban.main.model.AppSecret" >
    delete from app_secret_key
    where sign_key = #{signKey,jdbcType=VARCHAR}
  </delete>

  <insert id="insertAppSecret" parameterType="com.erban.main.model.AppSecret" >
    insert into app_secret_key (os, sign_key, app_version, create_time)
    values (#{os,jdbcType=INTEGER}, #{signKey,jdbcType=VARCHAR}, #{appVersion,jdbcType=VARCHAR}, #{createTime,jdbcType=TIMESTAMP})
  </insert>

</mapper>
